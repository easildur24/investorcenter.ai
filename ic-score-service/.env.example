# InvestorCenter.ai IC Score Service - Environment Variables
# Copy this file to .env and update with your configuration

# Database Configuration
DB_HOST=localhost
DB_PORT=5432
DB_USER=postgres
DB_PASSWORD=postgres
DB_NAME=investorcenter_db
DB_SSLMODE=prefer

# Connection Pool Settings
DB_POOL_SIZE=20
DB_MAX_OVERFLOW=10
DB_POOL_TIMEOUT=30
DB_POOL_RECYCLE=3600

# Debug Settings (development only - set to false in production)
DB_ECHO=false
DB_ECHO_POOL=false

# AI Analysis (Gemini)
# Get your API key from: https://aistudio.google.com/app/apikey
GEMINI_API_KEY=
